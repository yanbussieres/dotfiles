{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{ if or .Root .Segments.Git.RepoName }}[ {{ if .Root }}Admin{{ end }}{{ if and .Root .Segments.Git.RepoName }}@{{ end }}{{ if .Segments.Git.RepoName }}{{ .Segments.Git.RepoName }}{{ end }} ]{{ end }} {{ .Folder }}",
  "palette": {
    "background-color": "transparent",
    "cloud-text-gcp": "#4285F4",
    "cloud-text-firebase": "#FFA000",
    "error-background": "#FF9192",
    "error-text": "#242424",
    "git-text": "#238636",
    "kubernetes-text": "#FFBB00",
    "talosctl-text": "#FF6C00",
    "symbol-color": "#ffffff",
    "timer-text": "#FFAA93"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#81ff91",
          "properties": {
            "fetch_version": true
          },
          "style": "",
          "template": "{{if lt .PromptCount 4}}<#cc7eda> \u007C </><#7eb8da>\ue718</> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}{{end}}",
          "type": "node"
        },
        {
          "type": "npm",
          "style": "",
          "foreground": "#81ff91",
          "template": "{{if lt .PromptCount 4}}#cc9eda> \u007C </><#7eb8da>\ue71e </> {{ .Full }}{{end}} "
        },
        {
          "type": "dart",
          "style": "",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#06A4CE",
          "template": "  {{ .Full }} "
        },
        {
          "type": "elixir",
          "style": "",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#422251",
          "template": "  {{ .Full }} "
        },
        {
          "type": "flutter",
          "style": "",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#06A4CE",
          "template": "  {{ .Full }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "background": "p:background-color",
          "foreground": "p:cloud-text-gcp",
          "style": "",
          "template": " <p:symbol-color></> {{ .Project }}",
          "type": "gcp",
          "max-width": 3
        },
        {
          "background": "p:background-color",
          "foreground": "p:cloud-text-firebase",
          "style": "",
          "template": " <p:symbol-color>󰥧</> {{ .Project }}",
          "type": "firebase"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#193549",
          "background": "#FFDA6E",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFDA6E{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            },
            "source": "cli",
            "mapped_branches": {
              "feat/*": "🚀 ",
              "bug/*": "🐛 "
            }
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": false,
      "type": "rprompt",
      "segments": [
        {
          "background": "p:error-background",
          "foreground": "p:error-text",
          "style": "powerline",
          "template": "{{ if ne .Code 0 }}  {{ .Code }}{{ if (ne (reason .Code) (toString .Code)) }} - {{ reason .Code }}{{else}}{{ end }} {{ end }}",
          "type": "status"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "powerline",
          "background": "p:background-color",
          "foreground": "p:symbol-color",
          "powerline_symbol": "",
          "properties": {
            "style": "full",
            "mapped_locations": {
              "/Users/y/Dev/Ovet": "󰩄 ",
              "/Users/y/Dev/Ovet/sprinters/": "󰑮󰑮󰑮",
              "/Users/y/Dev/Ovet/team/": " "
            }
          },
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#523c3c",
    "template": "{{ .PromptCount }}> "
  }
}
